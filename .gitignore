.gradle
**/build/
!src/**/build/

# Ignore Gradle GUI config
gradle-app.setting

# Avoid ignoring Gradle wrapper jar file (.jar files are usually ignored)
!gradle-wrapper.jar

# Avoid ignore Gradle wrappper properties
!gradle-wrapper.properties

# Cache of project
.gradletasknamecache

# Eclipse Gradle plugin generated files
# Eclipse Core
.project
# JDT-specific (Eclipse Java Development Tools)
.classpath

# node_modules は Git にプッシュしない
node_modules/

# ビルド成果物（.next フォルダ）もプッシュしない
.next/

# ログファイル
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# 環境設定ファイル（秘密情報が含まれている場合）
.env*

# IDEの設定ファイル（例: VS Code）
.vscode/